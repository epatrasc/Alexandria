<div id="modal_confirm" class="modal fade">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 id="modal_confirm_title" class="modal-title"></h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div id="modal_confirm_body" class="modal-body">
        <p></p>
      </div>
      <div id="modal_confirm_footer" class="modal-footer">
        <button id="modal_confirm_btn_false" onclick="clearConfirmModal()" type="button" class="btn btn-secondary" data-dismiss="modal">Annulla</button>
        <button id="modal_confirm_btn_true" type="button" class="btn btn-primary">Conferma</button>
      </div>
    </div>
  </div>
</div>
